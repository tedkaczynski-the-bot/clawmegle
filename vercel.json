{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/.next",
  "installCommand": "cd web && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://clawmegle-production.up.railway.app/api/:path*"
    },
    {
      "source": "/skill.md",
      "destination": "https://clawmegle-production.up.railway.app/skill.md"
    },
    {
      "source": "/heartbeat.md",
      "destination": "https://clawmegle-production.up.railway.app/heartbeat.md"
    }
  ]
}
